{
  "name": "johan-berggren",
  "private": true,
  "description": "Official website for Johan Berggren",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "SASS_PATH=src/sass gatsby build",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "SASS_PATH=src/sass gatsby develop --port 3000",
    "serve": "SASS_PATH=src/sass gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@types/react-helmet": "^6.0.0",
    "gatsby": "^2.22.9",
